{
  "name": "full-node-auth",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k -p \"[{name} - {time}]\" -n \"TypeScript,Nodemon\" \"yarn watch-ts\" \"yarn watch-node\"",
    "build": "rm -rf dist && tsc",
    "build-test": "rm -rf dist && yarn install && tsc -p tests/tsconfig.json",
    "start": "node dist/index.js",
    "watch-node": "nodemon --delay 5 dist/index",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.16",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.5.2",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.5"
  },
  "_moduleAliases": {
    "@config": "dist/config",
  }
}
